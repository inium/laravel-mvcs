{
    "name": "inium/laravel-multier",
    "description": "Multi-tier Architecture (Layered architecture, Repository / Service / DTO) scaffolds on Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "repository", "service", "dto", "scaffolds", "multi-tier", "multi-tier architecture", "layered architecture"],
    "authors": [
        {
            "name": "inlee",
            "email": "einable@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "netresearch/jsonmapper": "^4.0",
        "symfony/http-foundation": "^6.0",
        "illuminate/support": "^9.27.0",
        "illuminate/database": "^9.27.0",
        "illuminate/pagination": "^9.27.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inium\\Multier\\MultierServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.1.0-dev"
        }
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Inium\\Multier\\": "src/"
        }
    }
}
