{
    "name": "inium/laravel-mvcs",
    "description": "Layered Architecture (Layered architecture, Repository / Service / DTO) scaffolds on Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "repository",
        "service",
        "dto",
        "scaffolds",
        "layered architecture"
    ],
    "authors": [
        {
            "name": "inlee",
            "email": "jamesinlee@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "netresearch/jsonmapper": "^4.0",
        "symfony/http-foundation": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inium\\Mvcs\\MvcsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.1.0-dev"
        }
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Inium\\Mvcs\\": "src/"
        }
    }
}
